{
  "name": "Windfinder Adblock",
  "description": "Extends the content to the edge of the screen, in order to remove the bottom ad bar.",
  "version": "1.0",
  "manifest_version": 3,

  "icons": { "16": "icons/icon16.png",
           "48": "icons/icon48.png",
          "128": "icons/icon128.png" },

  "content_scripts": [{
    "js": ["script.js"],
    "matches": ["https://*.windfinder.com/*"],
    "run_at": "document_end"
  }]
}
